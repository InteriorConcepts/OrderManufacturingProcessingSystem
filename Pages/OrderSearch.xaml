<Page x:Class="OMPS.Pages.OrderSearch"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:OMPS"
      xmlns:Components="clr-namespace:OMPS.Components" xmlns:pages="clr-namespace:OMPS.Pages"
      x:Name="Page_OrderSearch"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="JobSearch"
      Background="{DynamicResource {x:Static SystemColors.WindowTextBrushKey}}"
      Foreground="{DynamicResource {x:Static SystemColors.WindowBrushKey}}">

    <Grid DataContext="{Binding ElementName=Page_EngOrder}">
        <StackPanel>
            <TextBox></TextBox>
        </StackPanel>
        <DataGrid IsTextSearchEnabled="True" />
    </Grid>
</Page>

<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="wwwroot\**\*.*" CopyToOutputDirectory="Always" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="SQL_And_Config_Handler .Net8.dll" />
    <None Remove="TOML_Helper_.Net8.dll" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="SQL_And_Config_Handler .Net8.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="TOML_Helper_.Net8.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Web.WebView2" Version="1.0.3485.44" />
    <PackageReference Include="System.Data.OleDb" Version="9.0.8" />
    <PackageReference Include="Tomlyn" Version="0.19.0" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="SQL_And_Config_Handler .Net8">
      <HintPath>SQL_And_Config_Handler .Net8.dll</HintPath>
    </Reference>
    <Reference Include="TOML_Helper_.Net8">
      <HintPath>TOML_Helper_.Net8.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>